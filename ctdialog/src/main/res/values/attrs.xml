<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 上图片下文字 -->
    <declare-styleable name="ImageText">
        <attr name="itSrc" format="reference" />
        <attr name="itText" format="reference|string" />
        <attr name="textColor" format="reference|color" />
        <attr name="textSize" format="dimension" />
        <attr name="imageSize" format="dimension" />
    </declare-styleable>


    <declare-styleable name="CircleLoadingView">
        <attr name="max" format="integer" />
        <attr name="progress" format="integer" />
        <attr name="circleColor" format="color|reference" />
        <attr name="circleProgressColor" format="color|reference" />
        <attr name="circleWidth" format="dimension" />
        <attr name="cTextColor" format="color|reference" />
        <attr name="cTextSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CornerLayout">
        <attr name="radius" format="dimension" />
        <attr name="radiusTL" format="dimension" />
        <attr name="radiusTR" format="dimension" />
        <attr name="radiusBL" format="dimension" />
        <attr name="radiusBR" format="dimension" />
        <attr name="radiusTLX" format="dimension" />
        <attr name="radiusTRX" format="dimension" />
        <attr name="radiusBLX" format="dimension" />
        <attr name="radiusBRX" format="dimension" />
        <attr name="radiusTLY" format="dimension" />
        <attr name="radiusTRY" format="dimension" />
        <attr name="radiusBLY" format="dimension" />
        <attr name="radiusBRY" format="dimension" />
    </declare-styleable>
</resources>